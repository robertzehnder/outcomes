
  <%= form_for @user do |f| %>

    <p><%= f.label :email %>
    <%= f.text_field :email %></p>

    <p><%= f.label :password %>
    <%= f.password_field :password %></p>

    <p><%= f.label :image_url %>
    <%= f.text_field :image_url %></p>

    <p><%= f.label :homework_id %>
    <%= f.text_field :homework_id %></p>

    <p><%= f.submit "Submit" %></p>
    <% end %>
